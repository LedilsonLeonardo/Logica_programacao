programa {
	funcao inicio() 
	{
		inteiro x = 5
		inteiro y = 4 
		
		//Verifica se x é maior que y
		
		se(x > y)
		{
		     //Se for, será escrito que x é maior que y na tela
		     escreva ("X é maior que Y")
		}
		senao se (x == y)
		{
		    //Se for igual, x é igual a y
		    escreva("X  é igual a Y")
		}
		senao 
		{
		    //Senão, x é menor que y
		    escreva ("X é menor que Y")
		}
		
	}
}
