programa {
	funcao inicio()
	{
		inteiro idade = 19
		
		//Verificar se idade é menor que 18 anos
		se(idade < 18)
		{
		    escreva("Você  tem menos de 18 anos")
		}
		senao se (idade == 18)
		{
		    escreva ("Você tem 18 anos")
		}
		senao
		{
		    
		    escreva ("Você tem mais de 18 anos")
		}
		
	}
}
