programa {
	funcao inicio()
	{
	
	cadeia senha
	
	escreva ("Sistema Interno\n")
	
	escreva ("Digite a senha: ")
	leia(senha)
	
	//Verifica se a senha digitada é igual a 123adm
	se(senha == "123adm")
	{
	    escreva("Você acessou o sistema interno")
	}
	senao
	{
	    escreva ("Senha incorreta")
	}
	
		
	}
}
