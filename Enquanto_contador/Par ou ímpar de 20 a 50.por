/*
Par ou ímpar de 20 a 50
Complete o código corretamente para que ele percorra os 
números de 20 a 50 e informe se um número é par ou ímpar:
*/
programa 
{
	funcao inicio() 
	{
	inteiro contador = 20
	
	
	enquanto (contador <=50 )
	{
	    se (contador % 2 == 0 )
	 {   
	
	escreva (contador+" é par\n")
	
	     
	 }
	senao
	
	{
	    escreva (contador+" é impar\n")
	}
	contador = contador + 1
}
}

}
