programa
{
 funcao inicio()
 {
  inteiro destino

  escreva("Companhia Aérea\n\n")
  escreva("Qual o destino da viagem?\n\n")

  escreva("1 - Estados Unidos\n")
  escreva("2 - Argentina\n\n")

  leia(destino)

  se(destino == 1)
  {
   escreva("Você vai para os Estados Unidos\n")
  }

  se(destino == 2)
  {
   escreva("Você vai para a Argentina\n")
  }
 }
}