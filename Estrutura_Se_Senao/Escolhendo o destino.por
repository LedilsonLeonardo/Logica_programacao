	escreva ("1 - Itália\n")
	escreva ("2 - França\n")
	
	leia(destino)
	
	se (destino == 1)
	
	{
	    escreva ("Você vai para a Itália\n")
	}
	senao
	{
	    escreva ("Você vai para a França\n")
	}
	    
 }
